	<!-- 'Thank you' Responses-->
	<category>
		<pattern>THANK YOU</pattern>
		<template>
			<random>
				<li>No Problem :)</li>
				<li>You're welcome, happy to help :)</li>
				<li>You're welcome :)</li>
				<li>No Problem, always happy to help :)</li>			
			</random>
		</template>
	</category>
	
	<category>
		<pattern>THANK YOU _</pattern>
		<template>
			<srai>THANK YOU</srai>
		</template>
	</category>
	
	<category>
		<pattern>_ THANK YOU</pattern>
		<template>
			<srai>THANK YOU</srai>
		</template>
	</category>
	
	<category>
		<pattern>CHEERS</pattern>
		<template>
