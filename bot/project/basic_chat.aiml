<aiml version="1.0.1" encoding="UTF-8">

<!-- Basic chat responses -->

	<!-- 'Hello' Responses -->
    <category>
        <pattern>HELLO</pattern>
        <template>
			<random>
				<li>Hello! How can I help?</li>
				<li>Hey! How can I help?</li>
				<li>Hi! How can I help?</li>
				<li>Hello! How can I be of assistance?</li>
				<li>Hey! How can I be of assistance?</li>
				<li>Hi! How can I be of assistance?</li>
			</random>
        </template>
    </category>
    
	<category>
        <pattern>HELLO _</pattern>
        <template>
            <srai>
                HELLO
            </srai>
        </template>
    </category>
	
    <category>
        <pattern>HEY</pattern>
